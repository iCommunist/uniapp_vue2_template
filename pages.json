{
	"easycom": {
		"^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				// "navigationStyle": "custom",
				"navigationBarBackgroundColor": "#9ec8fc",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "标讯通",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/charts/charts",
			"style": {
				// "navigationStyle": "custom",
				"navigationBarBackgroundColor": "#9ec8fc",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "charts",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/sendCode/sendCode",
			"style": {
				// "navigationStyle": "custom",
				"navigationBarBackgroundColor": "#9ec8fc",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "sendCode",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/addImg/addImg",
			"style": {
				// "navigationStyle": "custom",
				"navigationBarBackgroundColor": "#9ec8fc",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "addImg",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				// "navigationStyle": "custom",
				"navigationBarBackgroundColor": "#9ec8fc",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "my",
				"enablePullDownRefresh": true
			}
		}
	],
	"subPackages": [
		{
			"root": "subpackages",
			"pages": [
				{
					"path" : "test1/test1",
					"style" : 
					{
						"navigationBarTitleText" : "test1",
						"navigationStyle": "custom",
						"enablePullDownRefresh":true 
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
	  // "custom": true,
	  "color": "#333333",
	  "selectedColor": "#6B1F72",
	  "borderStyle": "white",
	  "backgroundColor": "#ffffff",
	  "list": [
	    {
	      "pagePath": "pages/index/index",
	      "iconPath": "static/tabbar/icon_index.png",
